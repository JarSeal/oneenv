{
  "name": "oneenv",
  "version": "0.9.6",
  "description": "Distributes .env files from one master .env file to different folders/workspaces in a monorepo.",
  "main": "lib/main.js",
  "type": "module",
  "repository": "https://github.com/JarSeal/oneenv",
  "author": "JarSeal",
  "license": "MIT",
  "private": false,
  "bin": {
    "oneenv": "lib/main.js"
  },
  "keywords": [
    "env",
    ".env",
    "environment",
    "variables",
    "config",
    "settings"
  ],
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --verbose --runInBand --detectOpenHandles"
  },
  "dependencies": {
    "dotenv": "16.0.3"
  },
  "devDependencies": {
    "async": "3.2.4",
    "cross-env": "7.0.3",
    "jest": "29.5.0",
    "shelljs": "^0.8.5"
  },
  "engines": {
    "node": ">=16.13.2",
    "yarn": ">=1.22.15"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js"
    ],
    "modulePathIgnorePatterns": [
      "examples"
    ],
    "transform": {},
    "testTimeout": 30000
  }
}
